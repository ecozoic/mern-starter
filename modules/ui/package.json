    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "concurrently": "3.5.0",
    "css-loader": "0.28.5",
    "deep-freeze": "0.0.1",
    "documentation": "5.2.2",
    "enzyme": "2.9.1",
